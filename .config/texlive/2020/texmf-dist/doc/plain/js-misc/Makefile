	$(RM) *.log

export::
	touch site.imk
	$(MAKE) Makefile IMAKE_INCLUDES=-I.
	$(RM) -r site.imk Makefile.bak
	cp $(HOME)/Notes/Copying.gpl License
	pwd=`pwd` ; pwd=`suffix $$pwd /` ; cd .. ; \
	    gtar -czvf $$pwd.tar.gz \
		--exclude=CVS --exclude=.cvsignore $$pwd ; \
	    cp $$pwd/README $$pwd.README

